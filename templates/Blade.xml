    <context>
      <option name="HTML" value="true" />
      <option name="PHP" value="true" />
    </context>
  </template>
  <template name="bpp" value="@php&#10;    $END$&#10;@endphp" description="Blade @php" toReformat="false" toShortenFQNames="true">
